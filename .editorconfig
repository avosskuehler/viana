[*.cs]

# IDE0008: Expliziten Typ verwenden
csharp_style_var_elsewhere = false

# IDE0065: Die using-Anweisung wurde falsch platziert.
csharp_using_directive_placement = inside_namespace

# IDE0058: Der Ausdruckswert wird niemals verwendet.
csharp_style_unused_value_expression_statement_preference = unused_local_variable

# IDE0017: Initialisierung von Objekten vereinfachen
dotnet_style_object_initializer = false

# IDE0054: Verbundzuweisung verwenden
dotnet_style_prefer_compound_assignment = false
