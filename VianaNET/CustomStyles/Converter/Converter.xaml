<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:Viana="clr-namespace:VianaNET"
	>
  <Viana:DoubleToStringConverter x:Key="DoubleToStringConverter" />
  <Viana:PlusOneConverter x:Key="PlusOneConverter" />
  <Viana:UnitDoubleStringConverter x:Key="UnitDoubleStringConverter" />
  <Viana:InverseBooleanToVisibilityConverter x:Key="BoolVisibleConverterInverse" />
  <BooleanToVisibilityConverter x:Key="BoolVisibleConverter" />
  <Viana:DSTimeToMillisecondsConverter x:Key="DSTimeToMillisecondsConverter"/>
  <Viana:RulerUnitStringConverter x:Key="RulerUnitStringConverter"/>
  <Viana:PercentToDoubleConverter x:Key="PercentToDoubleConverter"/>

  <Viana:MeasurementType x:Key="TimeMeasurement">Time</Viana:MeasurementType>
  <Viana:MeasurementType x:Key="PixelMeasurement">Pixel</Viana:MeasurementType>
  <Viana:MeasurementType x:Key="PositionMeasurement">Position</Viana:MeasurementType>
  <Viana:MeasurementType x:Key="VelocityMeasurement">Velocity</Viana:MeasurementType>
  <Viana:MeasurementType x:Key="AccelerationMeasurement">Acceleration</Viana:MeasurementType>
</ResourceDictionary>